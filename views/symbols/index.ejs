        <% symbols.forEach(symbol =>{ %>
        <div class="well">
            <div class="forms">
                <h4><%=symbol.symbol%> - <%=symbol.name%></h4>
 
                <form action="/symbols/edit/<%=symbol.id%>" method="GET">
                <input type="submit" value="Edit">
                </form>
                <form action="/symbols/<%=symbol.id%>/?_method=DELETE" method="POST">
                <input type="submit" value="Delete">
                </form>
                
            </div>
        </div>
        <%})%>
